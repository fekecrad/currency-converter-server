{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"lib": ["es6"],
		"allowJs": true,
		"sourceMap": true,
		"rootDir": "./src",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"strictFunctionTypes": true,
		"noImplicitThis": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noImplicitReturns": true,
		"moduleResolution": "node",
		"esModuleInterop": true
	}
}
